CKSUMB	.BYT 0          ;B-PAGE CHECKSUM 
LBF0D	JMP ISVAR
.SKI
PATCHG	PHA
	LDA DSDESC
	BEQ PATH0
	PLA
	CMP DSDESC+2
	BNE PATH1
	JMP LBA70
PATH0	PLA
PATH1	JMP LBA74
.SKI
PATCHH	JSR CHRGET
	CMP #$FF
	BNE *+5
	JMP SNERR
	JMP CHRGOT
.SKI
PATCHI	LDA CHANNL
	BNE *+5
	JMP BUFFU1
	LDA CSTAT
	AND #$40
	BNE *+5
	JMP GETAGN
	JMP INPCON
	*=*+75
.FIL CODE10
