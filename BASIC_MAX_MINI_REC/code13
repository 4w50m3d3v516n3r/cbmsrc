.PAG 'CODE13'
FRE	LDA VALTYP
	BEQ NOFREF
	JSR FREFAC
NOFREF	JSR GARBA2
	SEC
	LDA FRETOP
	SBC STREND
	TAY
	LDA FRETOP+1
	SBC STREND+1
GIVAYF	LDX #0
	STX VALTYP
	STA FACHO
	STY FACHO+1
	LDX #144
	JMP FLOATS
POS	LDY PNTR
SNGFLT	LDA #0
	BEQ GIVAYF
ERRDIR	LDX CURLIN+1
	INX
	BNE DIMRTS
	LDX #ERRID
	JMP ERROR
DIMRTS	RTS
.END
