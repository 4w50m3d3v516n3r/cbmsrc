.PAGE 'ASMIO'

LDFA8	LDA #$0D
	LDY #$FF
LDFAC	INY
	JSR OUTDP1
	CPY IMAXCL
	BCC LDFAC
	LDY #0
	LDX ICOLP
LDFB8	LDA ICRD,X
	CPX IMAXCL
LDFBC	BCC LDFC0
	LDA #' '
LDFC0	STA HISTM,Y
	JSR OUTDP1
	INY
	INX
	CPY #5
	BNE LDFB8
LDFCC	LDA INFLG
	CMP #'U'
	BEQ LDFDA
	CMP #'T'
	BNE LDFE8
	JMP LOADTA

LDFDA	JMP (UIN)

LDFDD	JSR OUTALL
	INX
LDFE1	LDA MSGS,X
	CMP #';'
	BNE LDFDD
LDFE8	RTS

LDFE9	JSR LDFFA
OUTPUT	PHA
	TXA
	PHA
	JSR CRLF
	PLA
	TAX
	PLA
	RTS

	.BYTE $BF,$42,$BF,$DA

LDFFA	LDA #' '
	JMP OUTALL

	.BYTE $4E

.END
